public class coche {
    public static void main(String[] args) {
        coche1 micoche= new coch1();
        micoche.Aumentar();
        micoche.Aumentar();
        System.out.println(micoche.puertas);
    }

    static class coch1 {
       public int puertas=0;
        public  void Aumentar() {
            this.puertas++;
        }
    }
}
